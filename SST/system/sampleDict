type sets;
libs ("libsampling.so");

interpolationScheme cell;

setFormat    raw;

sets
(
	reed // name of the set
	{
		type face; // type of the set
		axis y;
		start (0.0 0.0 0.0);
		end (0.0 0.0261 0.0);
	}
);
fields
(
	p
);


